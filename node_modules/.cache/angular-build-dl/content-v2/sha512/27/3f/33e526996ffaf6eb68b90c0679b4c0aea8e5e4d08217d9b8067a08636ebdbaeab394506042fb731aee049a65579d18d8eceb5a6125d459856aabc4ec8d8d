.set({\r\n        \r\n        name:name,\r\n        email,\r\n        date:date,\r\n        people,\r\n        description,\r\n        cell:cell\r\n       \r\n      }).then(()=>{\r\n        res('')\r\n\r\n      }).catch((error)=>{\r\n        rej(error)\r\n\r\n      })\r\n    })\r\n\r\n  }\r\n\r\n\r\n  getLocation() {\r\n    //-28.74378920728236, 24.765178509039064\r\n    return new Promise((resolve, reject) => {\r\n      setTimeout(() => {\r\n        var geocoder = new google.maps.Geocoder;\r\n        var latlng = { lat:-28.74378920728236, lng:24.765178509039064};\r\n        geocoder.geocode({ 'location': latlng }, function (results, status) {\r\n          var address = results[0].address_components[3].short_name;\r\n           console.log(address);\r\n          console.log(results[0]);\r\n          resolve(address)\r\n        }, 8000);\r\n\r\n      })\r\n\r\n\r\n    })\r\n  }\r\n}\r\n\r\n","import { NgModule } from '@angular/core';\r\nimport { Routes, RouterModule } from '@angular/router';\r\n\r\nimport { FacilityBookingPage } from './facility-booking.page';\r\n\r\nconst routes: Routes = [\r\n  {\r\n    path: '',\r\n    component: FacilityBookingPage\r\n  }\r\n];\r\n\r\n@NgModule({\r\n  imports: [RouterModule.forChild(routes)],\r\n  exports: [RouterModule],\r\n})\r\nexport class FacilityBookingPageRoutingModule {}\r\n"]}